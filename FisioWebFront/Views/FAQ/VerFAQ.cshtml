@model IEnumerable<FisioWebFront.Entities.FaqObj>
@{
    ViewBag.Title = "VerFaq";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<body>
			<section class="page_breadcrumbs ls ms columns_margin_0">
				<div class="container">
					<div class="row">
						<div class="col-sm-12 ">
							<ol class="breadcrumb">
								<li>
									<a href="index.html">
										Inicio
									</a>
								</li>
								<li class="active">Preguntas frecuentes</li>
							</ol>
						</div>
					</div>
				</div>
			</section>


			<section class="ls section_padding_50 columns_padding_25">
				<div class="container">
					<div class="row">
						<div class="col-sm-12 text-center">
							<h2 class="section_header small">
								¿Tienes preguntas? Te tenemos las respuestas!
							</h2>
							<p>
								Esta sección te podría dar una guia de las preguntas más frecuentes que nos llegan a través de los distintos medios que tenemos de contacto pero no dude en agendar una cita si necesita más información y un diagnóstico adecuado
							</p>
						</div>
					</div>

					  @foreach (var item in Model)
                            {
					<div class="row vertical-tabs">
						<div class="col-sm-5">

							<!-- Nav tabs -->
							<ul class="nav" role="tablist">
								<li>
								<a href="#vertical-tab1" role="tab" data-toggle="tab"> @Html.DisplayFor(modelItem => item.pregunta_titulo)</a>
								</li>

							</ul>

						</div>

						<div class="col-sm-7">

							<!-- Tab panes -->
							<div class="tab-content no-border">
								<div class="tab-pane fade in active">
								<h3 class="topmargin_0">@Html.DisplayFor(modelItem => item.pregunta_titulo)</h3>
									<p>
									@Html.DisplayFor(modelItem => item.pregunta_respuesta)
									</p>
								</div>
	
						
							</div>

						</div>


					</div>
			}
				</div>
			</section>


	<script src="~/fluffypaw/js/compressed.js"></script>
	<script src="~/fluffypaw/js/main.js"></script>
	<script src="~/fluffypaw/js/switcher.js"></script>


</body>